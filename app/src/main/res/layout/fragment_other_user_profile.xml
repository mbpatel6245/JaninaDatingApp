<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/mainContainerFL"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="@color/black"
            app:layout_constraintBottom_toTopOf="@+id/ad_view"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <include
                android:id="@+id/appBarOtherProfile"
                layout="@layout/action_bar_layout" />

            <com.facebook.shimmer.ShimmerFrameLayout
                android:id="@+id/shimmerFrameLayoutOtherProfile"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:orientation="vertical"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/appBarOtherProfile">

                <include layout="@layout/other_profile_pre_design_layout" />

            </com.facebook.shimmer.ShimmerFrameLayout>

            <androidx.core.widget.NestedScrollView
                android:id="@+id/mainNSVOtherProfile"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:scrollbars="none"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/appBarOtherProfile">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/black">

                    <ImageView
                        android:id="@+id/backIV"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:background="@drawable/circle_round_boundary_red_bg"
                        android:elevation="1dp"
                        android:padding="@dimen/_10sdp"
                        android:rotation="180"
                        android:src="@drawable/ic_baseline_double_arrow_24"
                        android:visibility="gone"
                        app:layout_constraintBottom_toBottomOf="@id/viewPager"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="@id/viewPager" />

                    <ImageView
                        android:id="@+id/nextIV"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10sdp"
                        android:background="@drawable/circle_round_boundary_red_bg"
                        android:elevation="1dp"
                        android:padding="@dimen/_10sdp"
                        android:src="@drawable/ic_baseline_double_arrow_24"
                        android:visibility="gone"
                        app:layout_constraintBottom_toBottomOf="@id/viewPager"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@id/viewPager" />

                    <androidx.viewpager.widget.ViewPager
                        android:id="@+id/viewPager"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_250sdp"
                        android:layout_alignParentTop="true"

                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />


                    <ImageView
                        android:id="@+id/dislikeOtherProfileIV"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_12sdp"
                        android:layout_marginBottom="@dimen/_20sdp"
                        android:contentDescription="@string/app_name"
                        android:src="@drawable/close_1"
                        app:layout_constraintBottom_toBottomOf="@id/viewPager"
                        app:layout_constraintEnd_toStartOf="@id/likeOtherProfileIV"
                        app:layout_constraintHorizontal_chainStyle="packed"
                        app:layout_constraintStart_toStartOf="@id/viewPager"
                        app:layout_constraintTop_toBottomOf="@id/viewPager" />

                    <ImageView
                        android:id="@+id/likeOtherProfileIV"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_12sdp"
                        android:layout_marginBottom="@dimen/_20sdp"
                        android:contentDescription="@string/app_name"
                        android:src="@drawable/heart"
                        app:layout_constraintBottom_toBottomOf="@id/viewPager"
                        app:layout_constraintEnd_toEndOf="@id/viewPager"
                        app:layout_constraintHorizontal_chainStyle="packed"
                        app:layout_constraintStart_toEndOf="@id/dislikeOtherProfileIV"
                        app:layout_constraintTop_toBottomOf="@id/viewPager" />

                    <TextView
                        android:id="@+id/nameAndAgeOthersTV"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginHorizontal="@dimen/_20sdp"
                        android:layout_marginTop="@dimen/_12sdp"
                        android:gravity="center"
                        android:singleLine="true"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_18ssp"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/dislikeOtherProfileIV" />

                    <TextView
                        android:id="@+id/othersLocationTV"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="@dimen/_20sdp"
                        android:layout_marginTop="@dimen/_4sdp"
                        android:layout_marginBottom="@dimen/_minus4sdp"
                        android:drawablePadding="@dimen/_8sdp"
                        android:gravity="center"
                        android:singleLine="true"
                        android:textAlignment="gravity"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_10ssp"
                        app:drawableStartCompat="@drawable/location"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/nameAndAgeOthersTV" />

                    <TextView
